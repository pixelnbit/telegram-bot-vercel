{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/webhook.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/webhook.py"
    }
  ],
  "env": {
    "BOT_TOKEN": "@bot_token",
    "CHANNEL_USERNAME": "@channel_username",
    "OWNER_URL": "@owner_url",
    "OWNER_ID_1": "@owner_id_1",
    "OWNER_ID_2": "@owner_id_2",
    "WEBHOOK_URL": "@webhook_url"
  }
}
